{
    "^(.*((?<!\\w)|_)id)|nonce$": {
        "value_pattern": "^\"\\d{1,18}\"$",
        "replacement": "\"999999999999999999\""
    },
    "^((user)?name)|nick|description|content|label|summary|topic$": {
        "value_pattern": "^\".+\"$",
        "replacement": "\"none\""
    },
    "^(avatar|icon)$": {
        "value_pattern": "^\".+\"$",
        "replacement": "\"68b329da9893e34099c7d8ad5cb9c940\""
    },
    "^.*((?<!\\w)|_)url$": {
        "value_pattern": "^\".+\"$",
        "replacement": "\"https://www.example.org\""
    },
    "^(.*_?timestamp)|.+_at$": {
        "value_pattern": "^\".+\\+00:00\"$",
        "replacement": "\"1970-01-01T00:00:00.000000+00:00\""
    }
}
